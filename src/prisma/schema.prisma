datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  // id        Int     @id @default(autoincrement())
  id        String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  text      String
  completed Boolean @default(false)
}

model User {
  id String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email String
  name String 
  password String
}